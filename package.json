    "eslint": "^7.23.0",
    "eslint-plugin-cypress": "^2.11.2",
    "prettier": "^2.2.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0"
